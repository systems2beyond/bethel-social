rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /posts/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /groups/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /events/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /connect-form/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /member-registration/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
